{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node build/server.js",
    "start:dev": "tsx watch src/server.ts",
    "build": "tsup src --out-dir build",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "test:coverage": "vitest --coverage",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e-local": "vitest run --dir src/http",
    "pretest:e2e": "run-s test:create-prisma-env test:install-prisma-env",
    "test:create-prisma-env": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-env": "npm link vitest-environment-prisma",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.1.0",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.8.4",
    "prisma": "5.6.0",
    "tsup": "8.0.1",
    "tsx": "4.4.0",
    "typescript": "5.3.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@fastify/cookie": "9.2.0",
    "@fastify/jwt": "7.2.4",
    "@prisma/client": "5.6.0",
    "bcryptjs": "2.4.3",
    "dotenv": "16.3.1",
    "fastify": "4.24.3",
    "zod": "3.22.4"
  }
}
